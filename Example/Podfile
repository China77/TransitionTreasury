source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '8.0'

use_frameworks!

# Our Libraries
def libraries
  pod 'TransitionTreasury', :path => "../"
  pod 'TransitionAnimation', :path => "../"
end

target 'Example' do
  libraries
end

target 'ExampleWithoutAnimation' do
    libraries
end
